@model LibraryManagementSystem.Models.Entity.ADMIN

@{
    ViewBag.Title = "UpdateAdmin";
    Layout = "~/Views/Shared/Layout.cshtml";
}


@using (Html.BeginForm("UpdateAdmin", "Setting", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(c => c.ID)
        @Html.TextBoxFor(C => C.ID, new { @class = "form-control" })
        <br />
        <label>USERNAME</label>
        @Html.TextBoxFor(c => c.ADMINEMAIL, new { @class = "form-control" })
        <br />
        <label>PASSWORD</label>
        @Html.TextBoxFor(c => c.PASSWORD, new { @class = "form-control" })
        <br />
        <label>AUTHORITY</label>
        @Html.TextBoxFor(c => c.AUTHORITY, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Update</button>
    </div>
}


